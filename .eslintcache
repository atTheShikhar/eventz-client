y);\n            // console.log(data);\n            setEvents(data.events);\n            setPageData({\n                currentPage: data.currentPage,\n                totalPages: data.totalPages,\n                eventCount: data.eventCount\n            })\n        }\n        fetchEvents();\n        return function() {\n            setEvents(null);\n        }\n    },[load])\n\n    return (\n        <div className={classes.bgGrey}>\n        <Container maxWidth=\"lg\" className={classes.vpadding}>\n            {\n                (events!==null && events?.length !== 0) ? \n                (\n                    <>\n                        <Grid container spacing={2} >\n                            {\n                                events.map((item) => {\n                                    return (\n                                        <Grid \n                                            key={item._id} \n                                            item \n                                            xs={12} \n                                            sm={6} \n                                            md={4} \n                                        >\n                                            <EventCard\n                                                eventData={item}\n                                                clickRoute={`/event/${item._id}`}\n                                            />\n                                        </Grid>\n                                    );\n                                })\n                            }\n                        </Grid>\n\n                        <Divider variant=\"middle\" className={classes.vmargin}/>\n\n                        <div className={classes.divContainer}>\n                            <Pagination \n                                className={classes.pagination}\n                                count={pageData.totalPages}      \n                                onChange={(e,page) => {\n                                    setLoad(load => !load);\n                                }}\n                                renderItem={(item) => (\n                                    <PaginationItem\n                                        component={Link}\n                                        to={`/browse?page=${item.page}`}\n                                        {...item}\n                                    />\n                                )}\n                                shape=\"rounded\"\n                                size=\"large\"\n                            />\n                        </div>\n                    </>\n                ) :\n                (\n                    <NotFound/>\n                )\n            }\n        </Container>\n        </div>\n    )\n}\n\nexport default BrowseEvents\n","/home/s123/Desktop/project/eventzClient/src/components/navbar/Navbar.js",[],"/home/s123/Desktop/project/eventzClient/src/components/navbar/useStyles.js",[],"/home/s123/Desktop/project/eventzClient/src/components/navbar/NavList.js",[],"/home/s123/Desktop/project/eventzClient/src/components/navbar/menuItems.js",[],"/home/s123/Desktop/project/eventzClient/src/components/inputs/Textbox.js",[],"/home/s123/Desktop/project/eventzClient/src/components/cards/EventCard.js",["369"],"import { \n    Card, \n    CardActionArea,\n    CardActions, \n    CardContent, \n    CardMedia, \n    Chip, \n    Divider, \n    Typography,\n    Grid\n} from '@material-ui/core'\nimport LocationOnIcon from '@material-ui/icons/LocationOn';\nimport EventIcon from '@material-ui/icons/Event';\nimport ScheduleIcon from '@material-ui/icons/Schedule';\nimport React,{useState,useEffect} from 'react'\nimport noImage from '../../assets/no-image.jpg'\nimport useStyles from './CardStyle'\nimport { useHistory } from 'react-router';\nimport ProgressBar from '../dataDisplay/ProgressBar';\n\nfunction EventCard(props) {\n    const { title,genre,dateAndTime,isFree,price } = props.eventData.eventDetails;\n    const { stateName } = props.eventData.eventAddress;\n    const { imageLocation,totalTickets,bookedTickets } = props.eventData;\n    const { clickRoute } = props;\n    const date = new Date(dateAndTime).toDateString();\n    const time = new Date(dateAndTime).toLocaleTimeString();\n    let heading = title;\n    if(heading.length >= 20) {\n        heading = heading.substring(0,19) + \"...\";\n    }\n\n    const classes = useStyles();\n    const history = useHistory();\n    const [image,setImage] = useState(noImage);    \n    useEffect(() => {\n        if(imageLocation !== undefined) {\n            setImage(imageLocation);\n        }\n    },[])\n\n    const cardClickHandler = (ev) => {\n        history.push(clickRoute,{eventData: props.eventData, date: date, time: time, imgLink: image});\n    }\n\n    return (\n        <Card \n            raised={false}\n            variant=\"outlined\"\n            className={classes.card}\n        >\n            <CardActionArea onClick={cardClickHandler}> \n                <CardMedia\n                    className={classes.image}\n                    component=\"img\"\n                    src={image}\n                    title=\"Event Poster\"\n                />\n                <CardContent>\n                    <div className={classes.flexRow} style={{marginBottom: \"5px\"}}>\n                        <Typography\n                            gutterBottom\n                            variant=\"h5\"\n                            component=\"h2\"\n                            className={classes.heading}\n                        >\n                            {heading}\n                        </Typography>\n                        <Chip label={`${genre}`} variant=\"outlined\" color=\"secondary\"/>\n                    </div>\n\n                    <Chip \n                        label={`${time}`} \n                        icon={<ScheduleIcon/>} \n                        color=\"primary\"\n                        className={classes.chip}\n                    />\n                    <Chip \n                        label={`${date}`}\n                        icon={<EventIcon/>}\n                        color=\"primary\"\n                        className={classes.chip}\n                    />\n                    <br/>\n                    <Chip \n                        label={`${stateName}`} \n                        color=\"primary\" \n                        icon={<LocationOnIcon/>} \n                        className={classes.chip}\n                    />    \n\n                </CardContent>\n\n            </CardActionArea>\n\n            <Divider variant=\"middle\"/>\n\n            <CardActions className={classes.flexRow}>\n                    <div\n                        style={{width: \"80%\", padding: \"10px\"}}\n                    >\n                        <Grid container direction=\"row\" justify=\"space-between\" >\n                            <Grid item className={classes.subheading}>\n                                Tickets Booked: \n                            </Grid>\n                            <Grid item className={classes.subheading}>\n                                {bookedTickets} of {totalTickets}\n                            </Grid>\n                        </Grid>\n\n                        <ProgressBar\n                            value={(bookedTickets/totalTickets) * 100}\n                        />\n                    </div>\n                    <strong >\n                        {\n                            (isFree === \"No\") ?\n                            (<span className={classes.priceTag}>\n                                Rs: {price}\n                            </span>) : \n                            (<span className={classes.priceFreeTag}>FREE</span>)\n                        }\n                    </strong>                   \n            </CardActions>\n        </Card>\n    )\n}\n\nexport default EventCard\n","/home/s123/Desktop/project/eventzClient/src/components/cards/CardStyle.js",[],"/home/s123/Desktop/project/eventzClient/src/pages/EventPage.js",[],"/home/s123/Desktop/project/eventzClient/src/components/eventpage/Header.js",[],"/home/s123/Desktop/project/eventzClient/src/components/eventpage/Organiser.js",[],"/home/s123/Desktop/project/eventzClient/src/components/eventpage/Styles.js",[],"/home/s123/Desktop/project/eventzClient/src/components/eventpage/Description.js",[],"/home/s123/Desktop/project/eventzClient/src/components/eventpage/Address.js",["370"],"import useStyles from './Styles'\nimport { Container,Card,CardContent,Divider,Typography } from '@material-ui/core'\nimport FiberPinIcon from '@material-ui/icons/FiberPin';\nimport PublicIcon from '@material-ui/icons/Public';\nimport LocationCityIcon from '@material-ui/icons/LocationCity';\nimport LocationOnIcon from '@material-ui/icons/LocationOn';\nimport React from 'react'\n\nfunction Address(props) {\n    const {\n        apartment,\n        street,\n        district,\n        stateName,\n        pincode,\n        country\n    } = props.address;\n    const classes = useStyles();\n    return (\n        <Container className={classes.container}>\n            <Card\n                variant=\"outlined\"\n                raised={false}\n            >\n                <CardContent>\n                    <h3>Address of Event</h3>\n                    <Divider \n                        variant=\"middle\"\n                        className={classes.vmargin}\n                    />\n                    <div>\n                        <div className={classes.flexRow}>\n                            <LocationCityIcon color=\"primary\"/>\n                            <span className={classes.hmargin}>\n                                {apartment}, {street}\n                            </span>\n                        </div>\n                        <div className={`${classes.flexRow} ${classes.vmargin}`}>\n                            <LocationOnIcon color=\"primary\"/>\n                            <span className={classes.hmargin}>\n                                {district}, {stateName}\n                            </span>\n                        </div>\n                        <div className={classes.flexRow}>\n                            <FiberPinIcon color=\"primary\"/>\n                            <span className={classes.hmargin}>\n                                {pincode}\n                            </span>\n                        </div>\n                        <div className={`${classes.flexRow} ${classes.vmargin}`}>\n                            <PublicIcon color=\"primary\"/>\n                            <span className={classes.hmargin}>\n                                {country}\n                            </span>\n                        </div>\n                    </div> \n                </CardContent> \n            </Card>\n        </Container>\n    )\n}\n\nexport default Address\n","/home/s123/Desktop/project/eventzClient/src/pages/UploadEventPoster.js",[],"/home/s123/Desktop/project/eventzClient/src/components/navbar/UserMenu.js",[],"/home/s123/Desktop/project/eventzClient/src/pages/user/MyEvents.js",["371"],"import { Container, Divider, Grid } from '@material-ui/core'\nimport React, { useContext, useEffect } from 'react'\nimport CustomSelect from '../../components/inputs/CustomSelect'\nimport { DataContext } from '../../context/Context';\nimport EventCard from '../../components/cards/EventCard';\nimport useStyles from './Styles'\nimport { fetchDataAuth } from '../../helpers/fetchData';\nimport NotFound from '../../components/NotFound';\n\nfunction MyEvents(props) {\n    const classes = useStyles();\n    const {events,setEvents} = useContext(DataContext);\n    const selectData = [\"upcoming\",\"past\",\"pending\"]\n\n    useEffect(() => {\n        return function() {\n            setEvents(null);\n        }\n    },[])\n    const setEventData = (data) => {\n        setEvents(data.events)\n    }\n\n    r