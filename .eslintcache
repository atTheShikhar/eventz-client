n\" alignItems=\"center\">\n                <h1>My Events</h1>\n                <div className={`${classes.flex}`}>\n                    <CustomSelect \n                        page={1}\n                        url={\"/api/get-events-auth\"}\n                        selectData={selectData} \n                        selectHandler={setEventData} \n                        label=\"Type\"\n                        dataHandler={fetchDataAuth}\n                        />\n                </div>\n                </Grid>\n            </Container>\n\n            <Divider variant=\"middle\" className={classes.vmargin} />\n\n            <Container maxWidth=\"lg\" className={classes.flex}>\n                {\n                    (events!==null && events?.length !== 0) ? \n                    (\n                        <>\n                            <Grid container spacing={2}>\n                            {\n                                events.map((item) => {\n                                    return (\n                                        <Grid \n                                            key={item._id} \n                                            item \n                                            xs={12} \n                                            sm={6} \n                                            md={4} \n                                        >\n                                            <EventCard\n                                                eventData={item}\n                                                clickRoute={`/user/myevents/${item._id}`}\n                                            />\n                                        </Grid>\n                                    );\n                                })\n                            }\n                            </Grid>\n                        </>\n                    ) :\n                    (\n                        <>\n                            <NotFound/>\n                        </>\n                    )\n                }\n            </Container> \n        </div>\n    )\n}\n\nexport default MyEvents\n","/home/s123/Desktop/project/eventzClient/src/pages/user/Profile.js",[],"/home/s123/Desktop/project/eventzClient/src/components/inputs/CustomSelect.js",["368"],"import { FormControl, InputLabel, makeStyles, MenuItem, Select } from '@material-ui/core';\nimport React, { useEffect, useState, useContext} from 'react'\nimport {useHistory} from 'react-router'\nimport { ComponentContext } from '../../context/Context';\n\nconst useStyles = makeStyles(theme => ({\n    minWidth: {\n        minWidth: \"150px\"\n    }\n}))\n\nfunction CustomSelect(props) {\n    const {selectData,selectHandler,dataHandler,url,page} = props;\n    const [state,setState] = useState(selectData[0]);\n    const {setFeedback} = useContext(ComponentContext);\n    const classes = useStyles();\n    const history = useHistory();\n    useEffect(() => {\n        let fetchUrl = `${url}?type=${state}&page=${page}`\n        const getData = async () => {\n            const data = await dataHandler(fetchUrl,setFeedback,history);\n            selectHandler(data);\n        }\n        getData();\n    },[state])\n\n    const changeHandler = (e) => {\n        setState(e.target.value);\n    }\n\n    return (\n        <FormControl variant=\"outlined\" className={classes.minWidth}>\n            <InputLabel htmlFor=\"outlined-select-control\">{props.label}</InputLabel>\n            <Select\n                label={props.label}\n                onChange={changeHandler}\n                value={state}\n                inputProps={{\n                    id: 'outlined-select-control'\n                }}\n            >\n                {selectData.map((item) => (<MenuItem key={item} value={item}>{item}</MenuItem>))}\n            </Select>\n        </FormControl>\n    )\n}\n\nexport default CustomSelect\n","/home/s123/Desktop/project/eventzClient/src/pages/user/Styles.js",[],"/home/s123/Desktop/project/eve