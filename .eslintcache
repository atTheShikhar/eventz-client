 return (\n        <Card \n            // raised={false}\n            // variant=\"outlined\"\n            className={classes.card}\n        >\n            <CardActionArea onClick={cardClickHandler}> \n                <CardMedia\n                    className={classes.image}\n                    component=\"img\"\n                    src={image}\n                    title=\"Event Poster\"\n                />\n                <CardContent>\n                    <div className={classes.flexRow}>\n                        <h1 className={classes.heading}>\n                            {title}\n                        </h1>\n                    </div>\n\n                    <Divider variant=\"middle\" className={classes.vmargin}/>\n\n                    <h3 className={classes.subheading}>\n                        Total Tickets: <span style={{ color: \"#3f51b5\"}}>{noOfTickets}</span>\n\n                    </h3>\n                    <h2 className={classes.subheading}>\n                        <span>Price: </span>\n                        <strong>\n                            {\n                                (isFree === \"No\") ?\n                                (<>\n                                    {noOfTickets} * {price} = <span className={classes.priceTag}>{noOfTickets * price}</span>\n                                </>) : \n                                (<span className={classes.priceFreeTag}>FREE</span>)\n                            }\n                        </strong>                   \n                    </h2>\n\n                </CardContent>\n\n            </CardActionArea>\n\n        </Card>\n    )\n}\n\nexport default TicketCard\n","/home/s123/Desktop/project/eventzClient/src/pages/user/TicketPage.js",[],"/home/s123/Desktop/project/eventzClient/src/pages/admin/MonitorUsers.js",["376"],"/home/s123/Desktop/project/eventzClient/src/components/feedback/CustomDialog.js",[],"/home/s123/Desktop/project/eventzClient/src/pages/admin/UserPage.js",["377"],"import R