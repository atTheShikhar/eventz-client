rom '@material-ui/core';\nimport aboutUsImg from '../assets/about_us.svg'\nimport Copyright from '../components/Copyright'\nimport useStyles from './FormStyle';\n\nfunction About() {\n    const classes = useStyles();\n    return (\n        <div className={`${classes.page}`}>\n            <Container maxWidth=\"md\">\n                <Grid container spacing={4} className={classes.parent}>\n                    <Grid item xs={12} md={6}>\n                        <img className={classes.image} src={aboutUsImg} alt=\"Man reading texts\"/>\n                    </Grid>\n\n                    <Grid item xs={12} md={6} className={classes.content}>\n                        <h1 className={classes.heading}>\n                            About Us<span style={{ color: \"#2e4390\"}}>.</span>\n                        </h1>\n\n                        <Divider className={classes.vmargin} variant=\"middle\"/>\n\n                        <p className={classes.para}>\n                            <strong style={{ color: \"#2e4390\"}}>Eventz</strong> is an event management webapp built specifically\n                           for creating and booking professional events \n                           like Seminars, Conferences, Workshops etc.\n                           <br/>\n                           <br/>\n                           Organisers can give us their event details and we handle the \n                           tickets generation/verification, attendant information, revenue, etc. for them. \n                           We believe that organisers should not be charged for \n                           events that they are organising for free. Our service only charges you \n                           for the events that are paid, free events are managed for free.\n                           <br/>\n                           <br/>\n                           People can search events based on their interests and book tickets without any hassle.\n                           Tickets are in the form of QR code that can be scanned at the event venue entry.\n                        </p>\n                    </Grid>\n                </Grid>\n                <Box mt={7}>\n                    <Copyright />\n                </Box>\n            </Container>\n        </div>\n    )\n}\n\nexport default About\n","/home/s123/Desktop/project/eventzClient/src/pages/FormStyle.js",[],"/home/s123/Desktop/project/eventzClient/src/components/Copyright.js",[],"/home/s123/Desktop/project/eventzClient/src/pages/Create/AddressDetails.js",[],"/home/s123/Desktop/project/eventzClient/src/pages/Create/Styles.js",[],"/home/s123/Desktop/project/eventzClient/src/pages/Create/OrganiserDetails.js",[],"/home/s123/Desktop/project/eventzClient/src/components/buttons/SubmitButton.js",[],"/home/s123/Desktop/project/eventzClient/src/helpers/auth.js",[],"/home/s123/Desktop/project/eventzClient/src/pages/Create/EventDetails.js",[],"/home/s123/Desktop/project/eventzClient/src/helpers/validators.js",[],"/home/s123/Desktop/project/eventzClient/src/routes/PrivateRoute.js",[],"/home/s123/Desktop/project/eventzClient/src/pages/Activate.js",["256"],"import React, { useEffect, useState } from 'react'\nimport jwt_decode from 'jwt-decode';\nimport { Button, Paper, Container, Divider, Typography } from '@material-ui/core';\nimport useStyles from './FormStyle';\nimport StatusAndRedirect from '../components/StatusAndRedirect';\nimport { activateAccount } from '../helpers/auth';\n\n\nfunction Activate({match,history}) {\n    //Hooks\n    const classes = useStyles();\n    const [formData,setFormData] = useState({\n        name:\"\",\n        token: \"\",\n        show: \"activate\"\n    });\n    useEffect(() => {\n        const token =  match.params.token;\n        try {\n            const decoded = jwt_decode(token);\n            setFormData({\n                ...formData,\n                name: decoded.fname,\n                token\n            })\n        } catch(err) {\n            setFormData({\n                ...formData,\n                show: \"failed\"\n            })\n        }\n    },[])\n    //Functions \n    const confirmAccount = (e) => {\n        e.preventDefault();\n        //post handler\n        activateAccount(formData,setFormData,history);\n    }\n\n\n    //Render Logic\n    switch(formData.show) {\n        case \"activate\": return (\n            <div className={classes.page}>\n                <Container maxWidth=\"sm\">\n                    <Paper className={classes.card}>\n                        <Container className={classes.container}>\n                            <Typography\n                                variant=\"h4\"\n                                align=\"left\"\n                                color=\"textSecondary\"\n                            >\n                                Welcome <span className={classes.primaryColor}>\n                                    {formData.name}\n                                </span>,\n                            </Typography>\n                        </Container>\n                        <Container>\n                            <Typography\n                                align=\"left\"\n                                color=\"textSecondary\"\n                            >\n                                Please click the button below to confirm your account.\n                            </Typography>\n                            <Divider variant=\"middle\" />\n                        </Container>\n                        <Container className={classes.container}>\n                            <Button\n                                fullWidth\n                                variant=\"contained\"\n                                color=\"inherit\"\n                                onClick={confirmAccount}\n                                size=\"large\"\n                                className={classes.submit}\n                                style={{\n                                    backgroundColor: \"#20232a\",\n                                    color: \"white\"\n                                }}\n                            >\n                                Confirm Account\n                            </Button>\n                        </Container>\n                    </Paper>\n                </Container>\n            </div>\n        )\n        case \"failed\": return (\n            <StatusAndRedirect \n                header=\"Activation Failed\"\n                message=\"Link expired or invalid link!\"\n                type=\"failed\"\n            />\n        )\n        case \"success\": return (\n            <StatusAndRedirect \n                header=\"Activation Successful\"\n                message=\"Now you can login with your account!\"\n                type=\"success\"\n            />\n        )\n        case \"already exists\": return (\n            <StatusAndRedirect\n                header=\"Confirmed Already\"\n                message=\"Email already confirmed!\"\n                type=\"failed\"\n            />\n        )\n        default: return (<div></div>)\n    }\n}\n\nexport default Activate\n","/home/s123/Desktop/project/eventzClient/src/components/StatusAndRedirect.js",["257"],"import React, { useEffect, useState } from 'react'\nimport { Container, Typography, Card } from '@material-ui/core';\nimport useStyles from '../pages/FormStyle';\nimport { useHistory } from 'react-router-dom';\n\nfunction StatusAndRedirect(props) {\n    //Hooks\n    const classes = useStyles();\n    const history = useHistory();\n    const [seconds,setSeconds] = useState(5);\n    useEffect(() => {\n        const interval = setInterval(() => {\n            setSeconds((seconds) => seconds - 1)\n        }, 1000);\n\n        setTimeout(() => {\n            clearInterval(interval);\n            history.push('/');\n        },5000);\n    },[])\n\n    //Attributes\n    const { header, message, type } = props;\n    let headerColor, bgColor;\n    if (type === \"failed\") {\n        headerColor = classes.errorColor;\n        bgColor = classes.errorBg;\n    } else if(type === \"success\") {\n        headerColor = classes.successColor;\n        bgColor = classes.successBg;\n    } else {\n        headerColor = \"\";\n        bgColor = \"\";\n    }\n\n    //Render Logic\n    return (\n        <div className={classes.page}>\n            <Container maxWidth=\"sm\">\n                <Card className={`${classes.card} ${bgColor}`}>\n                    <Container className={classes.container}>\n                        <Typography\n                            variant=\"h4\"\n                            align=\"center\"\n                            className={headerColor}\n                        >\n                            {header}\n                        </Typography>\n                    </Container>\n                    <Container className={classes.container}>\n                        <Typography\n                            align=\"center\"\n                            color=\"textPrimary\"\n                        >\n                            {message}\n                        </Typography>\n                        <br/>\n                        <Typography\n                            align=\"center\"\n                            color=\"textPrimary\"\n                            variant=\"subtitle2\"\n                        >\n                            Redirecting to Homepage in {seconds} seconds...\n                        </Typography>\n                    </Container>\n                </Card>\n            </Container>\n        </div >\n    )\n}\n\nexport default StatusAndRedirect;\n","/home/s123/Desktop/project/eventzClient/src/pages/ForgetPassword.js",[],"/home/s123/Desktop/project/eventzClient/src/pages/ResetPassword.js",[],"/home/s123/Desktop/project/eventzClient/src/components/NetError.js",[],"/home/s123/Desktop/project/eventzClient/src/components/feedback/snackbar.js",[],"/home/s123/Desktop/project/eventzC